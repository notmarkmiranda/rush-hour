<div class="row">
	<div class="col-lg-12">
		<h1 class="page-header">
      By Specific URL: <%= "#{url.root}/#{url.path}" %>
    </h1>
  </div>
</div>
<h2>Statistics</h2>
<!--/.row-->
<!-- start of statistics -->
<div class="row">
  <div class="col-xs-12 col-md-6 col-lg-3">
    <div class="panel panel-blue panel-widget ">
      <div class="row no-padding">
        <div class="col-sm-3 col-lg-5 widget-left">
          <svg class="glyph stroked app-window-with-content"><use xlink:href="#stroked-app-window-with-content"></use></svg>
        </div>
        <div class="col-sm-9 col-lg-7 widget-right">
          <div class="large">
            <%= url.payload_requests.average_response_time %>
          </div>
          <div class="text-muted">
            Average Response Time
          </div>
        </div>
      </div>
    </div>
  </div>
	<div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-blue panel-widget ">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
					<svg class="glyph stroked app-window-with-content"><use xlink:href="#stroked-app-window-with-content"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="large">
            <%= url.payload_requests.max_response_time %>
          </div>
          <div class="text-muted">
            Max Response Time
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-blue panel-widget ">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
					<svg class="glyph stroked app-window-with-content"><use xlink:href="#stroked-app-window-with-content"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="large">
            <%= url.payload_requests.min_response_time %>
          </div>
          <div class="text-muted">
            Min Response Time
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-blue panel-widget ">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
					<svg class="glyph stroked app-window-with-content"><use xlink:href="#stroked-app-window-with-content"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="medium">
            <%= url.all_response_times_for_url_ordered.join(", ") %>
          </div>
          <div class="text-muted">
            All Response Times Ordered
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- row 2 -->
<div class="row">
  <div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-teal panel-widget">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
					<svg class="glyph stroked male-user"><use xlink:href="#stroked-bag"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="medium">
            <%= url.http_verbs_for_url.join(", ") %>
          </div>
					<div class="text-muted">
            HTTP Verbs for this URL
          </div>
				</div>
			</div>
		</div>
	</div>
  <div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-orange panel-widget">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
          <svg class="glyph stroked male-user"><use xlink:href="#stroked-male-user"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="small">
            <%= url.three_most_popular_referrers.join(", ") %>
          </div>
					<div class="text-muted">
            Three Most Popular Referrers
          </div>
				</div>
			</div>
		</div>
	</div>
  <div class="col-xs-12 col-md-6 col-lg-3">
		<div class="panel panel-red panel-widget">
			<div class="row no-padding">
				<div class="col-sm-3 col-lg-5 widget-left">
          <svg class="glyph stroked empty-message"><use xlink:href="#stroked-empty-message"></use></svg>
				</div>
				<div class="col-sm-9 col-lg-7 widget-right">
					<div class="small">
            <%=url.three_most_popular_user_agents.join("<br /> ")  %>
          </div>
					<div class="text-muted">
            Three Most Popular User Agents
          </div>
				</div>
			</div>
		</div>
	</div>
  <div class="col-xs-12 col-md-6 col-lg-3">
	</div>
  <div class="col-xs-12 col-md-6 col-lg-3">
  </div>
</div>
